<!--Javascript-->
<script>
    // Slider Script
    $(document).ready(function () {
        var swiperScroll = new Swiper('.swiper-container.c-carousel-srollbar', {
            slidesPerView: 'auto',
            spaceBetween: 17,
            loop: true,
            mousewheel: {
                forceToAxis: true,
                invert: false,
            },

            scrollbar: {
                el: '.swiper-scrollbar',
                hide: true,
            },
            breakpoints: {
                200: {
                    slidesPerView: 1.5,
                    spaceBetween: 15,
                },
                681: {
                    slidesPerView: 2.5,
                    spaceBetween: 15,
                },
                992: {
                    slidesPerView: 4.5,
                    spaceBetween: 20,
                }
            }

        });
    });
</script>

