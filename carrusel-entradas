    .c-arrow-slider,
    .swiper-button-prev.c-prevSlide img,
    .swiper-button-next.c-nextSlide img {
        margin: auto;
    }

    .is-breadcrumb-module {
        display: none !important;
    }

    body {
        background-color: #313032;
        font-family: 'Montserrat', sans-serif;
    }

    a {
        text-decoration: none;
    }

    .c-fullContent {
        width: 100%;
        padding: 0;
    }

    .c-relative {
        position: relative;
    }

    .m-top-15px {
        margin-top: 15px;
    }

    .m-bottom-15px {
        margin-bottom: 15px;
    }

    .swiper-slide.c-slide__scrollbar a .c-img-back {
        height: auto;
    }

    .c-divider {
        position: relative;
        width: 100%;
        border-bottom: 1px solid #EFBF7F;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .c-dividerTxt {
